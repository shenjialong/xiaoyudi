<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    version="2.5"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" >

    <servlet>
        <servlet-name>hello</servlet-name>
        <servlet-class>test.HelloWorldServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>hello</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>datacommit</servlet-name>
        <servlet-class>modle.DataCommitServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>datacommit</servlet-name>
        <url-pattern>/datacommit</url-pattern>
    </servlet-mapping>
    

    <welcome-file-list>
        <welcome-file>mchat.html</welcome-file>
    </welcome-file-list>

</web-app>